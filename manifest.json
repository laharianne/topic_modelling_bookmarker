{
  "manifest_version": 3,
  "name": "Website Categorizer",
  "version": "1.0",
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "hello_extensions.png",
      "48": "hello_extensions.png",
      "128": "hello_extensions.png"
    }
  },
  "permissions": ["bookmarks", "tabs", "notifications"],
  "host_permissions": [
    "http://*/*",
    "https://*/*",
    "http://127.0.0.1:5000/*"
  ],
  "background": {
    "service_worker": "background.js"
  }
}
